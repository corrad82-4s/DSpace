                     <cf:Term xml:lang="tr" trans="h">Uygulanabilir değil</cf:Term>
                     <cf:Definition xml:lang="en">Not Applicable	(or Unknown)</cf:Definition>
                  </cf:Class>
               </xs:appinfo>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
</xs:schema>
