               <xs:documentation xml:lang="tr">Uygulanabilir değil</xs:documentation>
               <xs:appinfo>
                  <cf:Class id="http://purl.org/coar/version/c_be7fb7dd8ff6fe43"
                            classSchemeId="http://purl.org/coar/version">
                     <cf:Term xml:lang="en" trans="o">NA</cf:Term>
