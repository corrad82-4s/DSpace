<dspace-dc-types>

    <dspace-header>
        <title>DSpace Cris Types</title>
    </dspace-header>

    <dc-schema>
        <name>cris</name>
        <namespace>https://www.4science.it/dspace-cris/</namespace>
    </dc-schema>

    <dc-type>
        <schema>cris</schema>
        <element>sourceId</element>
        <qualifier></qualifier>
        <scope_note>Used by the cris consumer to store the source that has originated the item</scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>policy</element>
        <qualifier>eperson</qualifier>
        <scope_note>Default metadata for custom policies rule</scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>policy</element>
        <qualifier>group</qualifier>
        <scope_note>Default metadata for custom policies rule</scope_note>
    </dc-type>
    
    <dc-type>
		<schema>cris</schema>
		<element>submission</element>
		<qualifier>definition</qualifier>
        <scope_note>Metadata to link a collection with a submission type</scope_note>
	</dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>owner</element>
        <qualifier></qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>access-token</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>refresh-token</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>scope</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>sync-mode</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>sync-publications</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>sync-projects</qualifier>
        <scope_note></scope_note>
    </dc-type>

    <dc-type>
        <schema>cris</schema>
        <element>orcid</element>
        <qualifier>sync-profile</qualifier>
        <scope_note></scope_note>
    </dc-type>

</dspace-dc-types>
